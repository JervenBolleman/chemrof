            owl:onProperty chemrof:composed_of ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Float ;
            owl:onProperty chemrof:maximum_percentage ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://UNKNOWN.org/ingredient_role_enum> ;
            owl:onProperty chemrof:has_role ],
        chemrof:RelationalChemicalEntity ;
    skos:definition "A part of a complex mixture that is of uniform composition" .

chemrof:Reaction a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "reaction" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Boolean ;
            owl:onProperty chemrof:is_balanced ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:reaction_center ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemrof:ReactionParticipant ;
            owl:onProperty chemrof:left_participants ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:smarts_string ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:description ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Boolean ;
            owl:onProperty chemrof:is_fully_characterized ],
        [ a owl:Restriction ;
            owl:onClass chemrof:ChemicalEncoding ;
            owl:onProperty chemrof:has_rinchi_representation ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Boolean ;
            owl:onProperty chemrof:is_transport ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:direction ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemrof:ReactionParticipant ;
            owl:onProperty chemrof:right_participants ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Boolean ;
            owl:onProperty chemrof:is_stereo ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Boolean ;
            owl:onProperty chemrof:is_diastereoselective ] ;
    skos:definition "reaction" .

chemrof:ReactionParticipant a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "reaction participant" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEntity ;
            owl:onProperty chemrof:participant ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:stoichiometry ],
        chemrof:RelationalChemicalEntity ;
    skos:definition "Instances of this link a reaction to a chemical entity participant" .

chemrof:SequenceInterval a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "sequence interval" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:SequenceString ;
            owl:onProperty chemrof:has_sequence_representation ],
        chemrof:MolecularComponent ;
    skos:definition "a chain of connected monomers in a linear polymer - for example, a sub-region of an amino acid or nucleotide sequence." ;
    skos:note "- includes an interval corresponding to the whole molecule as an edge case" .

chemrof:State a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "state" ;
    rdfs:subClassOf linkml:mixin .

chemrof:Stereocenter a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "stereocenter" ;
    rdfs:subClassOf chemrof:Location,
        chemrof:OwlClass ;
    skos:altLabel "stereogenic center" ;
    skos:definition "a particular instance of a stereogenic element that is geometrically a point, bearing different substituents, such that interchanging any two substituents leads to a stereoisomer" .

chemrof:Uncharged a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "uncharged" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:elemental_charge ],
        chemrof:ChargeState,
        linkml:mixin ;
    skos:altLabel "neutral charge" ;
    skos:exactMatch CHEMINF:000401 .

chemrof:chemical_isomer_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "chemical isomer of" ;
    rdfs:domain chemrof:Molecule ;
    rdfs:range chemrof:Molecule ;
    rdfs:seeAlso "https://en.wikipedia.org/wiki/Isomer" ;
    rdfs:subPropertyOf chemrof:alternate_form_of ;
    skos:definition "a relationship that holds between two forms of a parent molecule with identical chemical formulas but distinct arrangements of atoms in space" .

chemrof:decay_energy a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "decay energy" ;
    rdfs:domain chemrof:Isotope ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:chemical_property .

chemrof:decay_mode a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "decay mode" ;
    rdfs:domain chemrof:Isotope ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:chemical_property .

chemrof:decay_product a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "decay product" ;
    rdfs:domain chemrof:Isotope ;
    rdfs:range chemrof:Isotope ;
    rdfs:subPropertyOf chemrof:chemical_property .

chemrof:half_life a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "half life" ;
    rdfs:domain chemrof:Isotope ;
    rdfs:range chemrof:NumberOfYears ;
    rdfs:subPropertyOf chemrof:chemical_property .

chemrof:has_anionic_component a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:has_atom_occurrences a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:has_cationic_component a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:has_submolecules a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:hashed_chemical_formula a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "hashed chemical formula" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:chemical_representation ;
    skos:definition "A hashed version of a chemical formula" .

chemrof:id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:information_property .

chemrof:in_periodic_table_block a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:in_periodic_table_group a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:inchi_charge_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi charge sublayer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer .

chemrof:inchi_hydrogen_connections_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi hydrogen connections sublayer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer_in_main_layer ;
    skos:definition "Describes how many hydrogen atoms are connected to each of the other atoms." .

chemrof:inchi_isotopic_layer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi isotopic layer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer .

chemrof:inchi_stereochemical_double_bond_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi stereochemical double bond sublayer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer .

chemrof:inchi_stereochemical_type_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi stereochemical type sublayer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer .

chemrof:inchi_tetrahedral_stereochemical_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi tetrahedral stereochemical sublayer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer .

chemrof:mode_of_formation a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "mode of formation" ;
    rdfs:domain chemrof:Isotope ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:chemical_property .

chemrof:neutron_number a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "neutron number" ;
    rdfs:domain chemrof:Atom ;
    rdfs:range chemrof:Count ;
    rdfs:subPropertyOf chemrof:subatomic_particle_count ;
    skos:definition "number of neutrons in an atom" .

chemrof:rinchi_key_string a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "rinchi key string" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:hashed_chemical_formula ;
    skos:definition "A hashed representation of a RInChi string" .

chemrof:score a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:stereoisomer_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "stereoisomer of" ;
    rdfs:domain chemrof:Molecule ;
    rdfs:range chemrof:Molecule ;
    rdfs:subPropertyOf chemrof:chemical_isomer_of ;
    skos:definition "a relationship that holds between two forms of the 'same' molecule with identical chemical formulas but different bonds" .

chemrof:symbol a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:thermophysical_property a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "thermophysical property" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:chemical_property .

chemrof:AnionState a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "anion state" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:elemental_charge ],
        chemrof:Charged,
        linkml:mixin ;
    skos:altLabel "negatively charged state" ;
    skos:definition "This mixin is applied on an entity (atom or molecule) that is negatively charged" ;
    skos:exactMatch CHEMINF:000403 .

chemrof:CationState a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "cation state" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:elemental_charge ],
        chemrof:Charged,
        linkml:mixin ;
    skos:altLabel "positively charged state" ;
    skos:definition "This mixin is applied on an entity (atom or molecule) that is positively charged" ;
    skos:exactMatch CHEMINF:000402 .

chemrof:Connectivity a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "connectivity" ;
    rdfs:subClassOf linkml:mixin .

chemrof:FileLocation a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "file location" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] .

chemrof:GroupingClassForAcidsOrBases a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "grouping class for acids or bases" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEntity ;
            owl:onProperty chemrof:conjugate_acid_of ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEntity ;
            owl:onProperty chemrof:conjugate_base_of ],
        chemrof:MoleculeGroupingClass .

chemrof:InformationalChemicalEntity a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "informational chemical entity" .

chemrof:IsPartOrWhole a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "is part or whole" ;
    rdfs:subClassOf linkml:mixin .

chemrof:Nuclide a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "nuclide" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:energy_level ],
        chemrof:PartiallySpecifiedAtom ;
    skos:altLabel "nuclear species",
        "nucleide" ;
    skos:definition "an atomic species characterized by the specific constitution of its nucleus" ;
    linkml:examples "Example(value='https://en.wikipedia.org/wiki/Isotopes_of_tantalum#Tantalum-180m', description=None)" .

chemrof:NumberOfYears a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "number of years" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:int ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] .

chemrof:PartiallySpecifiedAtom a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "partially specified atom" ;
    rdfs:subClassOf chemrof:Atom ;
    skos:definition "An atom type that only has a subset of properties (has atomic number, charge, and neutron number) stated" .

chemrof:PreciseChemicalMixture a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "precise chemical mixture" ;
    rdfs:subClassOf chemrof:ChemicalMixture,
        chemrof:WeaklyBonded ;
    skos:altLabel "chemical substance (SIO)" ;
    skos:definition "A polyatomic entity that consists of two or more parts that are weakly bound, and of known stoichiometry" ;
    skos:exactMatch SIO:011126 .

chemrof:SubatomicParticle a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "subatomic particle" ;
    rdfs:subClassOf chemrof:ChemicalEntity ;
    skos:definition "A chemical entity below the granularity of an atom." ;
    skos:exactMatch CHEBI:36342,
        COB:0000007 .

chemrof:chemical_to_chemical_relationship a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "chemical to chemical relationship" ;
    rdfs:range linkml:String .

chemrof:conjugate_acid_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "conjugate acid of" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEntity ;
    rdfs:seeAlso "https://academic.oup.com/nar/article/36/suppl_1/D344/2506390" ;
    rdfs:subPropertyOf chemrof:has_derivative ;
    owl:inverseOf chemrof:conjugate_base_of .

chemrof:conjugate_base_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "conjugate base of" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEntity ;
    rdfs:seeAlso "https://academic.oup.com/nar/article/36/suppl_1/D344/2506390" ;
    rdfs:subPropertyOf chemrof:derivative_of ;
    owl:inverseOf chemrof:conjugate_acid_of ;
    skos:definition "connects the base form of an acid to an acid. Also connects the different charge states of the base. Example: the different charge states of lactate (CHEBI:24996) conjugate base of lactic acid (CHEBI:28358)" .

chemrof:has_element a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has element" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalElement ;
    rdfs:subPropertyOf chemrof:alternate_form_of .

chemrof:has_sequence_representation a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:inchi_atom_connections_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:inchi_chemical_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:smiles_string a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "smiles string" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:chemical_formula ;
    skos:definition "A string encoding of a molecular graph, no chiral or isotopic information. There are usually a large number of valid SMILES which represent a given structure. For example, CCO, OCC and C(O)C all specify the structure of ethanol." ;
    skos:note "a problematic set of structures to describe using SMILES notation is those which cannot be easily described using molecular graphs (see “Limitations of molecular graph representations” section), such as organometallic compounds and ionic salts." .

chemrof:subatomic_particle_count a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "subatomic particle count" ;
    rdfs:domain chemrof:Atom ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:property_of_atom .

chemrof:AtomOccurrence a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "atom occurrence" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:formal_charge ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:valence ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:oxidation_number ],
        [ a owl:Restriction ;
            owl:onClass chemrof:Atom ;
            owl:onProperty chemrof:occurrence_of ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:coordination_number ],
        chemrof:OwlClass,
        chemrof:RelationalChemicalEntity ;
    skos:definition "An occurrence of an atom in the context of a particular molecule. For example, one of two occurrences of oxygen in nitrate" .

chemrof:AtomicBond a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "atomic bond" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Float ;
            owl:onProperty chemrof:torsional_angle ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Float ;
            owl:onProperty chemrof:bond_angle ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Float ;
            owl:onProperty chemrof:bond_length_in_angstroms ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://UNKNOWN.org/bond_type_enum> ;
            owl:onProperty chemrof:bond_type ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemrof:AtomOccurrence ;
            owl:onProperty chemrof:has_atom_occurrences ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:bond_order ],
        chemrof:OwlClass,
        chemrof:RelationalChemicalEntity ;
    skos:altLabel "bond",
        "chemical bond" ;
    skos:definition "A connection between two atoms. Note this is the reified form of 'atomically connected to'. " ;
    skos:exactMatch CHEMINF:000063 .

chemrof:Charged a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "charged" ;
    rdfs:subClassOf chemrof:ChargeState,
        linkml:mixin ;
    skos:altLabel "ion",
        "ionic state" .

chemrof:PolyatomicEntity a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "polyatomic entity" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_atom_connections_sublayer ],
        chemrof:ChemicalEntity ;
    skos:definition "Any chemical entity consisting of more than one atom." .

chemrof:derivative_of a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:structural_isomer_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "structural isomer of" ;
    rdfs:domain chemrof:Molecule ;
    rdfs:range chemrof:Molecule ;
    rdfs:seeAlso "https://en.wikipedia.org/wiki/Structural_isomer",
        "https://goldbook.iupac.org/terms/view/C01285" ;
    rdfs:subPropertyOf chemrof:chemical_isomer_of ;
    skos:definition "a relationship that holds between two forms of the 'same' molecule with identical chemical formulas but different bonding patterns" .

chemrof:ChemicalElement a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "chemical element" ;
    rdfs:seeAlso "https://en.wikipedia.org/wiki/Chemical_element" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Float ;
            owl:onProperty chemrof:monoisotopic_mass ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:has_physiologically_stable_form ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:symbol ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Float ;
            owl:onProperty chemrof:mass ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:PeriodicTableBlock ;
            owl:onProperty chemrof:in_periodic_table_block ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Float ;
            owl:onProperty chemrof:melting_point_in_celcius ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:electron_configuration ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Float ;
            owl:onProperty chemrof:standard_atomic_weight ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Float ;
            owl:onProperty chemrof:boiling_point_in_celcius ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:PeriodicTableGroup ;
            owl:onProperty chemrof:in_periodic_table_group ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemrof:Stereocenter ;
            owl:onProperty chemrof:has_stereocenter ],
        chemrof:PartiallySpecifiedAtom ;
    skos:altLabel "element",
        "generic atom" ;
    skos:definition "generic form of an atom, with unspecified neutron or charge" ;
    skos:exactMatch wd:Q11344 ;
    linkml:examples "Example(value='carbon', description=None)",
        "Example(value='nickel', description=None)" .

chemrof:GroupingClass a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "grouping class" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom chemrof:GroupingClass ;
            owl:onProperty chemrof:subtype_of ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEntity ;
            owl:onProperty chemrof:classifies ],
        [ a owl:Restriction ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:id ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:OwlClass ;
            owl:onProperty chemrof:owl_subclass_of ],
        chemrof:OwlClass ;
    skos:definition "A non-specific entity. For example \"amino acid\" is a grouping for \"serine\", \"leucine\" etc; ester is a grouping for nitroglycerin; alkane is a grouping for pentane, butane, methane, etc" .

chemrof:RelationalChemicalEntity a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "relational chemical entity" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:OwlClass ;
            owl:onProperty chemrof:owl_subclass_of ],
        chemrof:OwlClass ;
    skos:closeMatch biolink:Association ;
    skos:definition "A reified relationship" .

chemrof:information_property a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "information property" ;
    rdfs:range linkml:String .

rdfs:label a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:MolecularComponent a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "molecular component" ;
    rdfs:subClassOf chemrof:PolyatomicEntity ;
    skos:altLabel "molecular part",
        "submolecule" ;
    skos:definition "A proper part of a molecule that is larger than an atom" .

chemrof:SpecificityMixin a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "specificity mixin" ;
    rdfs:subClassOf linkml:mixin .

chemrof:carboydrate_representation a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "carboydrate representation" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:chemical_representation .

chemrof:chemical_representation a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "chemical representation" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:information_property .

chemrof:subtype_of a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:Count a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "count" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:int ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] .

chemrof:Macromolecule a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "macromolecule" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom chemrof:Molecule ;
            owl:onProperty chemrof:has_submolecules ],
        chemrof:Molecule ;
    skos:definition "A large molecule, such as a polymer, lipid, or macrocycle" ;
    skos:exactMatch Wikipedia:Macromolecule ;
    skos:note "check on classification of lipids" .

chemrof:alternate_form_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "alternate form of" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEntity ;
    rdfs:subPropertyOf chemrof:chemical_to_chemical_relationship ;
    skos:definition "A relationship between two entities of the same type" .

chemrof:classifies a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:has_part a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:owl_subclass_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "owl subclass of" ;
    rdfs:domain chemrof:OwlClass ;
    rdfs:range chemrof:OwlClass ;
    rdfs:subPropertyOf chemrof:classification_relationship ;
    skos:definition "Holds between C and P if C owl:subClassOf P. Note that in this model, most classes are metaclasses (see 'owl class'), where these metaclasses may represent *groupings* or specific countable elements or species. We provide two different sub-properties of subclass of for two cases: (1) subtype_of, which connects grouping classes (e.g. alkane is_a hydrocarbon; alpha-amino-acid is_a amino-acid), (2) classified_by, which connects a specific element to a grouping class (e.g. methane is_a hydrocarbon; iron is_a metal; serine is_a amino-acid)" .

chemrof:chemical_property a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "chemical property" ;
    rdfs:range linkml:String .

linkml:TypeDefinition a owl:Class ;
    rdfs:label "type_definition" ;
    skos:definition "A data type definition." .

linkml:topValue a owl:DatatypeProperty ;
    rdfs:label "value" .

chemrof:MoleculeGroupingClass a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "molecule grouping class" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom chemrof:MoleculeGroupingClass ;
            owl:onProperty chemrof:subtype_of ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:Molecule ;
            owl:onProperty chemrof:classifies ],
        chemrof:GroupingClass ;
    skos:definition "A grouping class that classifies molecules. Example: carbohydrate, monosaccharide, amino acid, polyacrylamide" ;
    skos:note "Molecular grouping classes are typically defined by elemental composition and structure as well as chemical properties",
        "Unlike an actual molecule, a molecule grouping class will not have an inchi or properties such as mass" ;
    linkml:examples "Example(value='nucleobase', description=None)" .

chemrof:chemical_formula a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "chemical formula" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:chemical_representation ;
    skos:definition "A generic grouping for miolecular formulae and empirical formulae" .

chemrof:inchi_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi sublayer" ;
    rdfs:range linkml:String ;
    rdfs:seeAlso "https://www.inchi-trust.org/wp/wp-content/uploads/2014/05/techfaq_4.2.png" ;
    rdfs:subPropertyOf chemrof:inchi_component .

chemrof:Atom a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "atom" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:symbol ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:SubatomicParticle ;
            owl:onProperty chemrof:has_part ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:Count ;
            owl:onProperty chemrof:atomic_number ],
        chemrof:ChemicalEntity ;
    skos:altLabel "atomic species",
        "chemical species of an element" ;
    skos:definition "A chemical entity consisting of exactly one atomic nucleus and the electron(s) orbiting it." ;
    skos:exactMatch COB:0000011,
        SIO:010044 ;
    skos:note "{'chemical species of an element': 'Specific form of an element defined as to isotopic composition, electronic or oxidation state, and/or complex or molecular structure https://goldbook.iupac.org/terms/view/CT06859'}" .

chemrof:elemental_charge a owl:ObjectProperty,
        linkml:SlotDefinition .

chemrof:Isotope a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "isotope" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalElement ;
            owl:onProperty chemrof:has_element ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:NumberOfYears ;
            owl:onProperty chemrof:half_life ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:Isotope ;
            owl:onProperty chemrof:decay_product ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:decay_energy ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:decay_mode ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:Count ;
            owl:onProperty chemrof:neutron_number ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:mode_of_formation ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:Isotope ;
            owl:onProperty chemrof:isotope_of ],
        chemrof:Nuclide ;
    skos:definition "A specific subtype of an atom with a specified neutron number" ;
    skos:exactMatch wd:Q25276 ;
    skos:note "Wikidata has isotope as a subclass of nuclide, but nuclide has no instances that are not isotope instances." ;
    linkml:examples "Example(value='1H (protium)', description=None)",
        "Example(value='2H (deuterium)', description=None)",
        "Example(value='https://en.wikipedia.org/wiki/Carbon-12', description=None)" .

chemrof:OwlClass a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "owl class" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:OwlClass ;
            owl:onProperty chemrof:owl_subclass_of ],
        linkml:mixin ;
    skos:altLabel "metaclass",
        "second order class",
        "species" ;
    skos:closeMatch skos:Concept ;
    skos:definition "instances of this class mixin are OWL Classes" ;
    skos:exactMatch owl:Class,
        wd:Q24017414 .

chemrof:ChemicalEncoding a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "chemical encoding" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] .

chemrof:Molecule a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "molecule" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom chemrof:Atom ;
            owl:onProperty chemrof:has_atoms ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Boolean ;
            owl:onProperty chemrof:is_organic ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemrof:AtomicBond ;
            owl:onProperty chemrof:has_bonds ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemrof:Molecule ;
            owl:onProperty chemrof:has_submolecules ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:has_part ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemrof:AtomOccurrence ;
            owl:onProperty chemrof:has_atom_occurrences ],
        chemrof:FullyCovalentlyBonded,
        chemrof:PolyatomicEntity ;
    skos:altLabel "molecular entity",
        "monomolecular entity" ;
    skos:definition "A chemical entity that consists of two or more atoms where all atoms are connected via covalent bonds." ;
    skos:exactMatch COB:0000013,
        biotop:MonoMolecularEntity,
        SIO:011125 ;
    linkml:examples "Example(value='cysteine', description=None)",
        "Example(value='dioxygen', description=None)",
        "Example(value='phosphene', description=None)" .

chemrof:ChemicalEntity a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "chemical entity" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEncoding ;
            owl:onProperty chemrof:inchi_string ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Boolean ;
            owl:onProperty chemrof:is_radical ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalRole ;
            owl:onProperty chemrof:has_chemical_role ],
        [ a owl:Restriction ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:id ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty rdfs:label ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_chemical_sublayer ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:OwlClass ;
            owl:onProperty chemrof:owl_subclass_of ],
        chemrof:OwlClass ;
    skos:altLabel "chemical",
        "chemical species",
        "specific chemical entity" ;
    skos:definition "An entity that can be described using using chemical properties." ;
    skos:exactMatch CHEBI:24431,
        CHEMINF:000000,
        SIO:010004 ;
    skos:note "Does not encompass chemical grouping classes, such as \"amino acid\". Use the class ChemicalGrouping for this.",
        "Encompasses subatomic particles through to macromolecular structures.",
        "In common chemical usage generic and specific chemical names (such as radical or hydroxide ion) or chemical formulae refer either to a chemical species or to a molecular entity[https://goldbook.iupac.org/terms/view/CT01038]" .


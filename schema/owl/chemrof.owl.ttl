@prefix CHEBI: <http://purl.obolibrary.org/obo/CHEBI_> .
@prefix CHEMINF: <http://semanticscience.org/resource/CHEMINF_> .
@prefix COB: <http://purl.obolibrary.org/obo/COB_> .
@prefix IAO: <http://purl.obolibrary.org/obo/IAO_> .
@prefix SIO: <http://semanticscience.org/resource/SIO_> .
@prefix Wikipedia: <https://en.wikipedia.org/wiki/> .
@prefix biolink: <https://w3id.org/biolink/> .
@prefix biotop: <http://purl.org/biotop/biotop.owl#> .
@prefix chemrof: <https://w3id.org/chemrof/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pav: <http://purl.org/pav/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/chemrof> a owl:Ontology ;
    rdfs:label "chemrof" ;
    IAO:0000700 chemrof:ChemicalEntityOrGrouping,
        chemrof:Connectivity,
        chemrof:InformationalChemicalEntity,
        chemrof:IsPartOrWhole,
        chemrof:Material,
        chemrof:OwlClass,
        chemrof:Reaction,
        chemrof:SpecificityMixin,
        chemrof:State ;
    dcterms:license "https://creativecommons.org/publicdomain/zero/1.0/" ;
    dcterms:title "Chemical Entities Mixtures and Reactions Ontological Framework" ;
    pav:version "0.0.1" ;
    skos:definition """A data model for managing information about chemical entities, ranging from atoms
through molecules to complex mixtures.

Aspects of this have been cribbed from various sources including CHEBI, SIO,
Wikipedia/Wikidata, the NCATS Translator Chemical Working Group, but all mistakes
are my own.

For full context/motivation see the [GitHub repo](https://github.com/chemkg/chemrof).""" ;
    linkml:generation_date "2022-01-04T18:03:43" ;
    linkml:metamodel_version "1.7.0" ;
    linkml:source_file "chemrof.yaml" ;
    linkml:source_file_date "2022-01-04T17:48:54" ;
    linkml:source_file_size 68954 .

chemrof:AZE_notation_html a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "AZE notation html" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:chemical_representation ;
    skos:definition "E.g. <super>12</super>/<sub>6</sub>C" .

chemrof:AcidAnionGroupingClass a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "acid anion grouping class" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty rdfs:label ],
        chemrof:GroupingClassForAcidsOrBases ;
    skos:definition "A molecule grouping class that groups the different ion forms of the base of an acid" ;
    linkml:examples "Example(value='citrate', description=None)",
        "Example(value='pimelate', description=None)" .

chemrof:AcidBaseConflationClass a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "acid base conflation class" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:AcidBase ;
            owl:onProperty chemrof:has_physiological_base ],
        chemrof:GroupingClassForAcidsOrBases ;
    skos:definition "A molecule grouping class that can groups an acid together with all its conjugate bases. physiological" ;
    linkml:examples "Example(value='citrate/citric acid', description=None)" .

chemrof:Allotrope a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "allotrope" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass chemrof:ChemicalElement ;
            owl:onProperty chemrof:allotropic_analog_of ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:has_bonding_structure ;
            owl:qualifiedCardinality 1 ],
        chemrof:Molecule ;
    skos:definition "A molecule consisting of atoms of the same element" ;
    linkml:examples "Example(value='diamond', description=None)",
        "Example(value='dioxygen', description=None)" .

chemrof:AminoAcidSequenceInterval a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "amino acid sequence interval" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:AminoAcidSequenceString ;
            owl:onProperty chemrof:has_sequence_representation ],
        chemrof:SequenceInterval .

chemrof:AtomAnion a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "atom anion" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:elemental_charge ],
        chemrof:AnionState,
        chemrof:MonoatomicIon ;
    skos:definition "A monoatomic ion that is negative" .

chemrof:AtomCation a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "atom cation" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:elemental_charge ],
        chemrof:CationState,
        chemrof:MonoatomicIon ;
    skos:definition "A monoatomic ion that is positive, e.g Cu(2+)" ;
    linkml:examples "Example(value='Helium(2+)', description='1S/He/q+2')" .

chemrof:AtomGroupingByPeriodicTableBlock a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "atom grouping by periodic table block" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:PeriodicTableBlock ;
            owl:onProperty chemrof:in_periodic_table_block ],
        chemrof:AtomGroupingByPeriodicTablePlacement ;
    skos:definition "Example: group 13 atom" .

chemrof:AtomGroupingByPeriodicTableGroup a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "atom grouping by periodic table group" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:PeriodicTableGroup ;
            owl:onProperty chemrof:in_periodic_table_group ],
        chemrof:AtomGroupingByPeriodicTablePlacement ;
    skos:definition "Example: p-block atom" .

chemrof:AtomGroupingByProperty a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "atom grouping by property" ;
    rdfs:subClassOf chemrof:AtomGroupingClass ;
    skos:definition "Example: metal atom" .

chemrof:ChemicalGroupingByCharge a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "chemical grouping by charge" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty chemrof:has_charge_state ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemrof:MoleculeGroupingClass ;
            owl:onProperty chemrof:subtype_of ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEntity ;
            owl:onProperty chemrof:charge_agnostic_entity ],
        chemrof:GroupingClass ;
    skos:definition "A grouping class that refines a charge-neutral entity" ;
    linkml:examples "Example(value='copper cation', description=None)" .

chemrof:ChemicalSaltByAnion a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "chemical salt by anion" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty rdfs:label ],
        chemrof:ChemicalSaltGroupingClass ;
    skos:definition "A grouping class for a chemical salt named by anion" ;
    linkml:examples "Example(value='chloride salt', description=None)" .

chemrof:ChemicalSaltByCation a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "chemical salt by cation" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty rdfs:label ],
        chemrof:ChemicalSaltGroupingClass ;
    skos:definition "A grouping class for a chemical salt named by cation" ;
    linkml:examples "Example(value='sodium salt', description=None)" .

chemrof:ChiralityCenter a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "chirality center" ;
    rdfs:subClassOf chemrof:Stereocenter ;
    skos:definition "a stereocenter consisting of an atom holding a set of ligands (atoms or groups of atoms) in a spatial arrangement which is not superimposable on its mirror image. " .

chemrof:Copolymer a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "copolymer" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom chemrof:Macromolecule ;
            owl:onProperty chemrof:has_part ],
        chemrof:MonomolecularPolymer ;
    skos:definition "a polymer derived from more than one species of monomer" ;
    linkml:examples "Example(value='PEVA', description=None)" .

chemrof:DNASequenceInterval a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "DNA sequence interval" ;
    rdfs:subClassOf chemrof:NucleotideSequenceInterval .

chemrof:Electron a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "electron" ;
    rdfs:subClassOf chemrof:SubatomicParticle .

chemrof:EntityWithAtomsEnumerated a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "entity with atoms enumerated" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_chemical_sublayer ],
        chemrof:SpecificityMixin,
        linkml:mixin ;
    skos:definition "A mixin for an entity that consists of one or more atoms where the atoms and the number of occurrences is counted (corresponding to the chemical layer in InChI)" .

chemrof:EntityWithChargeSpecified a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "entity with charge specified" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_charge_sublayer ],
        chemrof:SpecificityMixin,
        linkml:mixin ;
    skos:definition "A mixin for an entity where the charge is explicitly specified (corresponding to the /q layer in InChI)" .

chemrof:EntityWithConnectivitySpecificied a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "entity with connectivity specificied" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_hydrogen_connections_sublayer ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_atom_connections_sublayer ],
        chemrof:SpecificityMixin,
        linkml:mixin ;
    skos:definition "A mixin for an entity that consists of two or more atom occurrences where the connectivity is specified (corresponding to the /c layer in InChI)" .

chemrof:EntityWithIsotopeSpecified a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "entity with isotope specified" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_isotopic_layer ],
        chemrof:SpecificityMixin,
        linkml:mixin ;
    skos:definition "A mixin for an entity where the charge is explicitly specified (corresponding to the /q layer in InChI)" .

chemrof:EntityWithStereochemistrySpecified a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "entity with stereochemistry specified" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_stereochemical_double_bond_sublayer ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_tetrahedral_stereochemical_sublayer ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_stereochemical_type_sublayer ],
        chemrof:SpecificityMixin,
        linkml:mixin ;
    skos:definition "A mixin for an entity where the stereochemistry is explicitly specified (corresponding to the /t, m/, and /s layers in InChI)" .

chemrof:Ester a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "ester" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEntity ;
            owl:onProperty chemrof:has_parent_alcohol ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:BronstedAcid ;
            owl:onProperty chemrof:has_parent_acid ],
        chemrof:Molecule ;
    skos:definition "An ester is a chemical compound derived from an acid (organic or inorganic) in which at least one –OH hydroxyl group is replaced by an –O– alkyl (alkoxy) group" ;
    linkml:examples "Example(value='butyl ethanoate', description='has parent alcohol butanol and has parent acid acetic acid')" .

chemrof:FullySpecifiedAtom a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "fully specified atom" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:Count ;
            owl:onProperty chemrof:neutron_number ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:elemental_charge ],
        chemrof:Atom ;
    skos:altLabel "atomic species",
        "chemical species of an element" ;
    skos:definition "An atom type that has atomic number, charge, and neutron number stated (equivalently: protons + mass + charge)" ;
    skos:note "{'chemical species of an element': 'Specific form of an element defined as to isotopic composition, electronic or oxidation state, and/or complex or molecular structure https://goldbook.iupac.org/terms/view/CT06859'}" ;
    linkml:examples "Example(value='Helium-4(2+)', description='The atom that is equivalent to an alpha particle 1S/He/q+2/i1+0')" .

chemrof:FunctionalGroup a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "functional group" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:Molecule ;
            owl:onProperty chemrof:is_substitutent_group_from ],
        chemrof:MolecularComponent .

chemrof:GeneralAcidBaseGroupingClass a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "general acid base grouping class" ;
    rdfs:subClassOf chemrof:GroupingClassForAcidsOrBases ;
    skos:definition "A molecule grouping class that can groups an acid together with all its conjugate bases. physiological" ;
    linkml:examples "Example(value='tricarboxylic acid/tricarboxylic acid anion', description=None)" .

chemrof:GeneralizedMolecularStructureClass a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "generalized molecular structure class" ;
    rdfs:subClassOf chemrof:MoleculeGroupingClass ;
    skos:definition "A molecule grouping class that can be written using a chemical formula using variables (e.g. n), or arithmetic expressions uing variables (e.g. 2n+2), plus optionally constraints on those variables (e.g. n>3). An example is alkane; CnH2n+2, which groups specific molecules such as methane" ;
    linkml:examples "Example(value='monosaccharide', description=None)" .

chemrof:Glycan a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "glycan" ;
    rdfs:subClassOf chemrof:Macromolecule ;
    skos:altLabel "polysaccharides" ;
    skos:definition "A macromolecule consisting of a large number of monosaccharides linked glycosidically" ;
    skos:note "Glycans composed of a single type of monosaccharide residue (homopolysaccharide, synonym homoglycan) are named by replacing the ending '-ose' of the sugar by '-an', e.g. mannans, fructans, xylans, arabinans" ;
    linkml:examples "Example(value='https://www.glycoepitope.jp/epitopes/EP0311', description='GalNAc(b1-4)[Neu5Ac(a2-3)]Gal(b1)')" .

chemrof:IsPart a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "is part" ;
    rdfs:subClassOf chemrof:IsPartOrWhole .

chemrof:IsWhole a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "is whole" ;
    rdfs:subClassOf chemrof:IsPartOrWhole .

chemrof:Isobar a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "isobar" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:Isotope ;
            owl:onProperty chemrof:decay_product ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:decay_mode ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:decay_energy ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:Count ;
            owl:onProperty chemrof:nucleon_number ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:mode_of_formation ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalElement ;
            owl:onProperty chemrof:has_element ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:NumberOfYears ;
            owl:onProperty chemrof:half_life ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEntity ;
            owl:onProperty chemrof:isobar_of ],
        chemrof:Nuclide ;
    skos:definition "Isobars are atoms (nuclides) of different chemical elements that have the same number of nucleons." ;
    linkml:examples "Example(value='isobar-40', description='Any nuclide with a mass number of 40:  40S, 40Cl, 40Ar, 40K, and 40Ca')" .

chemrof:IsomeraseReaction a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "isomerase reaction" ;
    rdfs:subClassOf chemrof:Reaction ;
    skos:definition "A reaction that converts a molecule from one isomer to another." .

chemrof:Lipid a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "lipid" ;
    rdfs:subClassOf chemrof:Macromolecule .

chemrof:Moiety a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "moiety" ;
    rdfs:subClassOf chemrof:MolecularComponent ;
    skos:definition "a named part of a molecule. In some instances moieties may be composed of yet smaller moieties and functional groups" .

chemrof:MolecularCation a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "molecular cation" ;
    rdfs:subClassOf chemrof:CationState,
        chemrof:PolyatomicIon ;
    skos:definition "A polyatomic ion that is positive" .

chemrof:MolecularDerivativeGroupingClass a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "molecular derivative grouping class" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEntity ;
            owl:onProperty chemrof:derivative_of ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty rdfs:label ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:Molecule ;
            owl:onProperty chemrof:classifies ],
        chemrof:MoleculeGroupingClass ;
    skos:definition "A grouping class defined as a derivative of another molecule or molecular grouping class" ;
    linkml:examples "Example(value='D-ornithine derivative', description=None)",
        "Example(value='oxoacid derivative', description=None)" .

chemrof:MolecularSpecies a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "molecular species" ;
    rdfs:subClassOf chemrof:Molecule ;
    skos:definition "A molecule in which the units are identical. Example: methyl" .

chemrof:MolecularSubsequence a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "molecular subsequence" ;
    rdfs:subClassOf chemrof:PolymerPart .

chemrof:MoleculeGroupingClassDefinedByAdditionOfAGroup a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "molecule grouping class defined by addition of a group" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom chemrof:ChemicalGroup ;
            owl:onProperty chemrof:has_group ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:PolyatomicEntity ;
            owl:onProperty chemrof:derivative_of ],
        chemrof:MoleculeGroupingClass ;
    skos:definition "A grouping class defined in terms of a derivative of a molecule and the addition of a group" ;
    linkml:examples "Example(value='acetyl xylan', description=None)" .

chemrof:MoleculeGroupingClassDefinedByComponents a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "molecule grouping class defined by components" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEntityOrGrouping ;
            owl:onProperty chemrof:has_part ],
        chemrof:MoleculeGroupingClass ;
    skos:definition "A grouping class defined as an exhaustive list of components" ;
    linkml:examples "Example(value='nucleoside', description='A nucleoside consists of a nucleobase and a five-carbon sugar (ribose or 2-deoxyribose)')",
        "Example(value='nucleotide', description='a nucleotide is composed of a nucleobase, a five-carbon sugar, and one or more phosphate groups')" .

chemrof:Monomer a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "monomer" ;
    rdfs:subClassOf chemrof:PolymerPart ;
    skos:altLabel "momomer species" .

chemrof:Nanostructure a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "nanostructure" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://UNKNOWN.org/nanostructure_morphology_enum> ;
            owl:onProperty chemrof:has_morphological_category ],
        chemrof:Material ;
    skos:definition "A nanostructure is a structure of intermediate size between microscopic and molecular structures. Nanostructural detail is microstructure at nanoscale." ;
    linkml:examples "Example(value='Zinc oxide (ZnO) nanorod', description='A nanostructure that has rod morphology and is composed of zinc oxide')",
        "Example(value='platinum copper nanoparticle', description='A nanostructure that is made from platinum and copper atoms')" .

chemrof:NaturalProduct a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "natural product" ;
    rdfs:subClassOf chemrof:Molecule .

chemrof:Neutron a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "neutron" ;
    rdfs:subClassOf chemrof:Nucleon .

chemrof:NonSpeciesMolecule a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "non species molecule" ;
    rdfs:subClassOf chemrof:Molecule ;
    skos:definition "A molecule in which the units are not identical. TODO: move this higher" .

chemrof:Peptide a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "peptide" ;
    rdfs:subClassOf chemrof:Macromolecule .

chemrof:Protein a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "protein" ;
    rdfs:subClassOf chemrof:Macromolecule .

chemrof:Proton a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "proton" ;
    rdfs:subClassOf chemrof:Nucleon .

chemrof:RNASequenceInterval a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "RNA sequence interval" ;
    rdfs:subClassOf chemrof:NucleotideSequenceInterval .

chemrof:RacemicMixture a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "racemic mixture" ;
    rdfs:seeAlso "https://github.com/ebi-chebi/ChEBI/issues/3245" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass chemrof:Enantiomer ;
            owl:onProperty chemrof:has_left_enantiomer ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:Molecule ;
            owl:onProperty chemrof:chirality_agnostic_form ],
        [ a owl:Restriction ;
            owl:onClass chemrof:Enantiomer ;
            owl:onProperty chemrof:has_right_enantiomer ;
            owl:qualifiedCardinality 1 ],
        chemrof:PreciseChemicalMixture ;
    skos:altLabel "racemate",
        "racemic mixture of enantiomers" ;
    skos:definition "a chemical compound that has equal amounts of left- and right-handed enantiomers of a chiral molecule. An example is Thalidomide" ;
    skos:exactMatch CHEBI:60911 .

chemrof:Radionuclide a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "radionuclide" ;
    rdfs:subClassOf chemrof:Nuclide ;
    skos:altLabel "radioactive isotope",
        "radioactive nuclide" ;
    skos:definition "an atom that has excess nuclear energy, making it unstable" ;
    linkml:examples "Example(value='radioactive isotope', description=None)" .

chemrof:SmallMolecule a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "small molecule" ;
    rdfs:subClassOf chemrof:Molecule ;
    skos:definition "A molecule characterized by availability in small-molecule databases" .

chemrof:StandardInchiObject a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "standard inchi object" ;
    rdfs:seeAlso "https://en.wikipedia.org/wiki/International_Chemical_Identifier" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_chemical_sublayer ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_tetrahedral_stereochemical_sublayer ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_stereochemical_double_bond_sublayer ],
        [ a owl:Restriction ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_version_string ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_charge_sublayer ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_hydrogen_connections_sublayer ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_proton_sublayer ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_isotopic_layer ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_stereochemical_type_sublayer ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_atom_connections_sublayer ],
        chemrof:InchiObject .

chemrof:SupramolecularPolymer a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "supramolecular polymer" ;
    rdfs:subClassOf chemrof:MolecularComplex ;
    skos:definition "polymeric arrays of monomeric units that are connected by reversible and highly directional secondary interactions–that is, non-covalent bonds" ;
    skos:exactMatch Wikipedia:Supramolecular_polymer .

chemrof:TanimotoSimilarity a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "tanimoto similarity" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Float ;
            owl:onProperty chemrof:score ],
        chemrof:MoleculePairwiseSimilarity ;
    skos:altLabel "Jaccard similarity",
        "Tanimoto coefficient" .

chemrof:UnchargedAtom a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "uncharged atom" ;
    rdfs:subClassOf chemrof:AtomIonicForm,
        chemrof:Uncharged ;
    skos:definition "An atom type that has no charge" ;
    skos:exactMatch COB:0000043 .

chemrof:UnchargedMolecule a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "uncharged molecule" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:elemental_charge ],
        chemrof:Molecule,
        chemrof:Uncharged ;
    skos:definition "A molecule that has no charge" .

chemrof:atomically_connected_to a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "atomically connected to" ;
    rdfs:domain chemrof:AtomOccurrence ;
    rdfs:range chemrof:AtomOccurrence .

chemrof:bond_energy a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "bond energy" ;
    rdfs:domain chemrof:AtomicBond ;
    rdfs:range linkml:Float ;
    skos:definition "the measure of bond strength in a chemical bond." .

chemrof:bond_length a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "bond length" ;
    rdfs:domain chemrof:AtomicBond ;
    rdfs:range linkml:Float ;
    skos:definition "the average distance between nuclei of two bonded atoms in a molecule" .

chemrof:canonical_smiles_string a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "canonical smiles string" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:smiles_string ;
    skos:definition "A deterministic SMILES string. Note there is no unique system for canonical smiles strings" .

chemrof:carbbank_representation a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "carbbank representation" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:carboydrate_representation ;
    skos:definition "The carbohydrate structure format used by the Complex Carbohydrate Structure Database (CCSD) originally developed at the University of Georgia. This is a 2D structure where bars and hyphens are used to indicate linkages, and monosaccharides are specified in the following order, separated by hyphens: anomer - configuration symbol - monosaccharide abbreviation." .

chemrof:classified_by a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "classified by" ;
    rdfs:domain chemrof:OwlClass ;
    rdfs:range chemrof:OwlClass ;
    rdfs:subPropertyOf chemrof:chemical_to_chemical_relationship,
        chemrof:owl_subclass_of ;
    owl:inverseOf chemrof:classifies ;
    skos:definition "A subtype_of relationship holds between I and C if I is a specific entity of type C. Here I and C denotes instances of OWL classes, where I is a specific class and C is a grouping class" ;
    linkml:examples "Example(value=None, description='glucose classified by monosaccharide')" .

chemrof:curly_smiles_string a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "curly smiles string" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:seeAlso "http://www.axeleratio.com/csm/proj/main.htm" ;
    rdfs:subPropertyOf chemrof:chemical_formula ;
    skos:definition "CurlySMILES is a chemical line notation which extends SMILES with annotations for storage, retrieval and modeling of interlinked, coordinated, assembled and adsorbed molecules in supramolecular structures and nanodevices. https://jcheminf.biomedcentral.com/articles/10.1186/1758-2946-3-1" .

chemrof:empirical_formula a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "empirical formula" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:chemical_formula ;
    skos:definition "A chemical formula where numbers denote relative proportion of atoms. E.g. CH2O for glucose" .

chemrof:enantiomer_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "enantiomer of" ;
    rdfs:domain chemrof:Molecule ;
    rdfs:range chemrof:Molecule ;
    rdfs:subPropertyOf chemrof:stereoisomer_of ;
    skos:definition "a stereoisomer relationship where the two forms are mirror images of one another" .

chemrof:epimer_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "epimer of" ;
    rdfs:domain chemrof:Molecule ;
    rdfs:range chemrof:Molecule ;
    rdfs:subPropertyOf chemrof:diastereoisomers_of ;
    skos:definition "A diastereoisomer of relationship in which the isomers differ at only one stereocenter" .

chemrof:extended_smiles_string a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "extended smiles string" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:smiles_string .

chemrof:functional_isomer_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "functional isomer of" ;
    rdfs:domain chemrof:Molecule ;
    rdfs:range chemrof:Molecule ;
    rdfs:subPropertyOf chemrof:structural_isomer_of ;
    skos:definition "structural isomers that have the same molecular formula (that is, the same number of atoms of the same elements), but the atoms are connected in different ways so that the groupings are dissimilar." .

chemrof:generalized_empirical_formula a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "generalized empirical formula" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:chemical_formula ;
    skos:definition "A variant of an empirical formula where numbers are replaced by generic ns; e.g. CnH2n+2 for alkane" .

chemrof:glycoct_condensed a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "glycoct condensed" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:carboydrate_representation ;
    skos:definition "This is a multiline format used by GlycomeDB, among others, and uses a graph notation to represent glycans, similar to KCF. Monosaccharides are specified more strictly to be able to map the monosaccharides between different representations. Substituents, such as N-acetyl, are specified as separate nodes from their basetypes and are linked together in the edge section. If newlines are removed, this format can be made linear." .

chemrof:glycominds_linearcode_representation a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "glycominds linearcode representation" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:carboydrate_representation ;
    skos:definition "A linear notation proposed by GlycoMinds, Ltd., where monosaccharides are indicated by a one- or two-letter code, linkages are indicated by “a” or “b” for anomers and a single number for the carbon number linkage on the reducing end, and branches are indicated by parentheses. This format is the most compact among existing formats, and several databases use a modified form of this format to accommodate additional codes and varying carbohydrate structures (such as ambiguous structures)." .

chemrof:has_cyclic_structure a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has cyclic structure" ;
    rdfs:domain chemrof:Molecule ;
    rdfs:range linkml:Boolean ;
    rdfs:subPropertyOf chemrof:chemical_property ;
    skos:definition "true if the molecule possesses a cycle in the atom connectivity graph" .

chemrof:has_ionic_component a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has ionic component" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:has_part .

chemrof:has_molfile_location a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has molfile location" ;
    rdfs:range chemrof:FileLocation ;
    rdfs:subPropertyOf chemrof:information_property ;
    skos:definition "Location of a MolFile representing a chemical structure" .

chemrof:has_rauxinfo_representation a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has rauxinfo representation" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:seeAlso "https://jcheminf.biomedcentral.com/articles/10.1186/s13321-018-0277-8" ;
    rdfs:subPropertyOf chemrof:chemical_representation ;
    skos:definition "Auxhiliary information on top of a RInChi" ;
    skos:note "A half-reaction, such as a RInChI with starting materials and no identified products, is a valid RInChI" .

chemrof:has_subtype a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has subtype" ;
    rdfs:domain chemrof:OwlClass ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:chemical_to_chemical_relationship ;
    owl:inverseOf chemrof:subtype_of .

chemrof:inchi_fixed_H_layer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi fixed H layer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer .

chemrof:inchi_key_string a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi key string" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:hashed_chemical_formula ;
    skos:definition "A hashed representation of an InChi string" .

chemrof:inchi_reconnected_layer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi reconnected layer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer .

chemrof:isomeric_smiles_string a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "isomeric smiles string" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:smiles_string ;
    skos:definition "A SMILES string that distinguishes between isomeric forms. E.g. [13C] for carbon-13" .

chemrof:isotone_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "isotone of" ;
    rdfs:domain chemrof:Isotope ;
    rdfs:range chemrof:Isotope ;
    rdfs:subPropertyOf chemrof:alternate_form_of ;
    skos:definition "relationship that holds between two isotones, always holds if the isotones have a different atomic number and the same neutron number." .

chemrof:kegg_chemical_function_representation a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "kegg chemical function representation" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:carboydrate_representation ;
    skos:definition "A multiline format used by the KEGG GLYCAN database and represents glycans as graphs. Monosaccharides are represented as nodes, and glycosidic linkages are represented as edges. Each node is given a unique number, and x- and y- coordinates are specified such that the glycan structure can be drawn on a 2D plane. Edge information uses the node numbers to specify anomeric and carbon number information. Monosaccharide names are specified as text, and no particular rules are specified to represent these names." .

chemrof:linucs_representation a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "linucs representation" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:carboydrate_representation ;
    skos:definition "A unique linear notation proposed and used by GLYCOSCIENCES.de, with the reducing end specified on the left. Monosaccharides, written according to CarbBank rules, are surrounded by square brackets and anomers, and stereochemistry is specified and separated by hyphens. Glycosidic linkages are also surrounded by hyphens, and carbon numbers of the linkages are separated by the plus sign. Branches are indicated by curly brackets." .

chemrof:long_rinchi_key_string a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "long rinchi key string" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:rinchi_key_string .

chemrof:lychi_string a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "lychi string" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:seeAlso "https://github.com/ncats/lychi" ;
    rdfs:subPropertyOf chemrof:chemical_formula .

chemrof:molar_cohesive_energy a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "molar cohesive energy" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:thermophysical_property .

chemrof:molar_value a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "molar value" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:thermophysical_property .

chemrof:molecular_formula a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "molecular formula" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:chemical_formula ;
    skos:definition "A chemical formula where numbers denote atom count. E.g. C6H12O6  for glucose" .

chemrof:next_in_homologous_series_from a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "next in homologous series from" ;
    rdfs:domain chemrof:Molecule ;
    rdfs:range chemrof:Molecule ;
    rdfs:seeAlso "https://en.wikipedia.org/wiki/Homologous_series" ;
    skos:definition "Next in a sequence of compounds with the same functional group and similar chemical properties in which the members of the series can be branched or unbranched. E.g.  ethane (C2H6) next in homologous series from methane (CH4)" .

chemrof:nuclear_isomer_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "nuclear isomer of" ;
    rdfs:domain chemrof:Isotope ;
    rdfs:range chemrof:Isotope ;
    rdfs:subPropertyOf chemrof:alternate_form_of ;
    skos:definition "relationship that holds between two nuclear isomers, always holds if the isomers have the same nucleon number but different energy states" .

chemrof:position_isomer_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "position isomer of" ;
    rdfs:domain chemrof:Molecule ;
    rdfs:range chemrof:Molecule ;
    rdfs:subPropertyOf chemrof:structural_isomer_of ;
    skos:definition "a functional group or other substituent changes position on a parent structure." .

chemrof:selfies_string a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "selfies string" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:seeAlso "https://iopscience.iop.org/article/10.1088/2632-2153/aba947" ;
    rdfs:subPropertyOf chemrof:chemical_formula .

chemrof:short_rinchi_key_string a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "short rinchi key string" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:rinchi_key_string .

chemrof:skeletal_isomer_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "skeletal isomer of" ;
    rdfs:domain chemrof:Molecule ;
    rdfs:range chemrof:Molecule ;
    rdfs:subPropertyOf chemrof:structural_isomer_of ;
    skos:definition "components of the (usually carbon) skeleton are distinctly re-ordered to create different structures. Pentane exists as three isomers: n-pentane (often called simply \"pentane\"), isopentane (2-methylbutane) and neopentane (dimethylpropane)." .

chemrof:tautomer_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "tautomer of" ;
    rdfs:domain chemrof:Molecule ;
    rdfs:range chemrof:Molecule ;
    rdfs:subPropertyOf chemrof:structural_isomer_of ;
    skos:definition "structural isomers (constitutional isomers) of chemical compounds that readily interconvert. Examples: a neutral amino acid and its zwitterion" .

chemrof:wurcs_representation a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "wurcs representation" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:carboydrate_representation .

linkml:SubsetDefinition a owl:Class ;
    rdfs:label "subset_definition" ;
    skos:definition "the name and description of a subset" .

chemrof:AminoAcidSequenceString a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "amino acid sequence string" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] .

chemrof:ChemicalGroup a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "chemical group" ;
    rdfs:subClassOf chemrof:MolecularComponent ;
    skos:altLabel "chemical functional group",
        "functional group",
        "group" .

chemrof:ChemicalSalt a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "chemical salt" ;
    rdfs:seeAlso "https://github.com/MaastrichtU-IDS/semanticscience/issues/96" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:CationState ;
            owl:onProperty chemrof:has_cationic_component ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:elemental_charge ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:AnionState ;
            owl:onProperty chemrof:has_anionic_component ],
        chemrof:PreciseChemicalMixture ;
    skos:definition "a chemical compound consisting of an ionic assembly of cations and anions." ;
    skos:exactMatch SIO:001422 ;
    skos:note "in SIO falls under molecular complex? Also in biolink" ;
    linkml:examples "Example(value='sodium carbonate CHEBI:29377', description=None)" .

chemrof:DNASequenceString a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "DNA sequence string" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] .

chemrof:FullyCovalentlyBonded a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "fully covalently bonded" ;
    rdfs:subClassOf chemrof:Connectivity ;
    skos:definition "All pairs of atoms in this entity are transitively connected via covalent bonds" .

chemrof:ImpreciseChemicalMixture a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "imprecise chemical mixture" ;
    rdfs:seeAlso "https://jcheminf.biomedcentral.com/articles/10.1186/s13321-019-0357-4" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEncoding ;
            owl:onProperty chemrof:has_minchi_representation ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:FileLocation ;
            owl:onProperty chemrof:has_mixfile_location ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemrof:ProportionalPart ;
            owl:onProperty chemrof:has_proportional_parts ],
        chemrof:ChemicalMixture ;
    skos:altLabel "portion of matter" ;
    skos:definition "A macroscopic polyatomic entity that consists of multiple chemical entities where the stoichiometry is not specified and the spatial arrangement of the ingredients is not defined. Examples include coal, tea tree oil, toothpaste, organometallic compounds, heterogeneous metals" ;
    skos:note "although the stoichiometry is not known rough percentage parts can be provided" ;
    linkml:examples "Example(value='toothpaste (sodium fluoride 0.243 w/v%[active] + H2O[inactive] + ... )', description=None)" .

chemrof:InchiObject a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "inchi object" ;
    rdfs:seeAlso "https://doi.org/10.1186/s13321-015-0068-4" ;
    rdfs:subClassOf chemrof:InformationalChemicalEntity ;
    skos:definition "A representation of an InChI string as an object, composed of distinct sublayers" .

chemrof:Location a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "location" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:OwlClass ;
            owl:onProperty chemrof:owl_subclass_of ],
        chemrof:OwlClass .

chemrof:MolecularAnion a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "molecular anion" ;
    rdfs:subClassOf chemrof:AnionState,
        chemrof:PolyatomicIon ;
    skos:definition "A polyatomic ion that is negative" .

chemrof:MolecularComplex a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "molecular complex" ;
    rdfs:subClassOf chemrof:PreciseChemicalMixture ;
    skos:altLabel "complex of molecular entities",
        "molecule complex",
        "poly molecular composite entity" ;
    skos:definition "A molecular complex is a chemical complex composed of at least one weakly interacting molecule.[SIO] Examples: chromosome, double-standed DNA, protein-containing macromolecular complex" ;
    skos:exactMatch COB:0000080,
        biotop:MoleculeComplex,
        SIO:010506 ;
    skos:note "In SIO, molecular complex is an only child of chemical complex, which is under chemical substance. chemical complex is defined as A chemical complex is a chemical substance composed of a weakly connected molecules and ions in a known stoichiometry.",
        "In bioptop, molecule complex is a sibling of structured biological entity under poly molecular composite entity" .

chemrof:MolecularComponentGroupingClass a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "molecular component grouping class" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:MolecularComponent ;
            owl:onProperty chemrof:classifies ],
        [ a owl:Restriction ;

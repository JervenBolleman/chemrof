    rdfs:range chemrof:NumberOfYears ;
    rdfs:subPropertyOf chemrof:chemical_property .

chemrof:has_anionic_component a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has anionic component" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:has_ionic_component .

chemrof:has_atom_occurrences a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has atom occurrences" ;
    rdfs:range chemrof:AtomOccurrence .

chemrof:has_cationic_component a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has cationic component" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:has_ionic_component .

chemrof:has_different_charge_state_from a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has different charge state from" ;
    rdfs:range linkml:String .

chemrof:has_ionic_component a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has ionic component" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:has_part .

chemrof:hashed_chemical_formula a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "hashed chemical formula" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:chemical_representation ;
    skos:definition "A hashed version of a chemical formula" .

chemrof:id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:information_property ;
    skos:exactMatch schema:identifier .

chemrof:in_periodic_table_block a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "in periodic table block" ;
    rdfs:range chemrof:PeriodicTableBlock .

chemrof:in_periodic_table_group a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "in periodic table group" ;
    rdfs:range chemrof:PeriodicTableGroup .

chemrof:inchi_charge_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi charge sublayer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer .

chemrof:inchi_component a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi component" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:information_property ;
    skos:definition "A part of an InChI object" .

chemrof:inchi_hydrogen_connections_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi hydrogen connections sublayer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer_in_main_layer ;
    skos:definition "Describes how many hydrogen atoms are connected to each of the other atoms." .

chemrof:inchi_isotopic_layer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi isotopic layer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer .

chemrof:inchi_stereochemical_double_bond_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi stereochemical double bond sublayer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer .

chemrof:inchi_stereochemical_type_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi stereochemical type sublayer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer .

chemrof:inchi_tetrahedral_stereochemical_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi tetrahedral stereochemical sublayer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer .

chemrof:mass a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "mass" ;
    rdfs:range linkml:Float .

chemrof:mode_of_formation a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "mode of formation" ;
    rdfs:domain chemrof:Isotope ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:chemical_property .

chemrof:neutron_number a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "neutron number" ;
    rdfs:domain chemrof:Atom ;
    rdfs:range chemrof:Count ;
    rdfs:subPropertyOf chemrof:subatomic_particle_count ;
    skos:altLabel "N" ;
    skos:definition "number of neutrons in an atom" ;
    skos:exactMatch CHEMINF:000078 .

chemrof:property_of_atom a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "property of atom" ;
    rdfs:domain chemrof:Atom ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:chemical_property .

chemrof:rinchi_key_string a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "rinchi key string" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:hashed_chemical_formula ;
    skos:definition "A hashed representation of a RInChi string" .

chemrof:score a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "score" ;
    rdfs:range linkml:String .

chemrof:stereoisomer_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "stereoisomer of" ;
    rdfs:domain chemrof:Molecule ;
    rdfs:range chemrof:Molecule ;
    rdfs:subPropertyOf chemrof:chemical_isomer_of ;
    skos:altLabel "spatial isomer of" ;
    skos:definition "a relationship that holds between two forms of the 'same' molecule with identical chemical formulas but different bonds" .

chemrof:thermophysical_property a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "thermophysical property" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:chemical_property .

chemrof:AnionState a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "anion state" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:elemental_charge ],
        chemrof:Charged,
        linkml:mixin ;
    skos:altLabel "negatively charged state" ;
    skos:definition "This mixin is applied on an entity (atom or molecule) that is negatively charged" ;
    skos:exactMatch CHEMINF:000403 .

chemrof:CationState a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "cation state" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:elemental_charge ],
        chemrof:Charged,
        linkml:mixin ;
    skos:altLabel "positively charged state" ;
    skos:definition "This mixin is applied on an entity (atom or molecule) that is positively charged" ;
    skos:exactMatch CHEMINF:000402 .

chemrof:Connectivity a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "connectivity" ;
    rdfs:subClassOf linkml:mixin ;
    skos:definition "Abstract grouping for slots that related chemical entities via a connectivity relationships" .

chemrof:ElementMetallicClassification a owl:Class,
        linkml:EnumDefinition ;
    rdfs:label "element_metallic_classification" ;
    owl:unionOf ( damlpt:Metallic damlpt:Non-Metallic damlpt:Semi-Metallic ) ;
    linkml:permissible_values damlpt:Metallic,
        damlpt:Non-Metallic,
        damlpt:Semi-Metallic .

chemrof:FileLocation a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "file location" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] .

chemrof:GroupingClassForAcidsOrBases a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "grouping class for acids or bases" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEntity ;
            owl:onProperty chemrof:conjugate_acid_of ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEntity ;
            owl:onProperty chemrof:conjugate_base_of ],
        chemrof:MoleculeGroupingClass .

chemrof:InformationalChemicalEntity a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "informational chemical entity" .

chemrof:IsPartOrWhole a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "is part or whole" ;
    rdfs:subClassOf linkml:mixin .

chemrof:Nuclide a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "nuclide" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:energy_level ],
        chemrof:PartiallySpecifiedAtom ;
    skos:altLabel "nuclear species",
        "nucleide" ;
    skos:definition "an atomic species characterized by the specific constitution of its nucleus" ;
    linkml:examples "Example(value='https://en.wikipedia.org/wiki/Isotopes_of_tantalum#Tantalum-180m', description=None)" .

chemrof:NumberOfYears a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "number of years" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:int ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] .

chemrof:PartiallySpecifiedAtom a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "partially specified atom" ;
    rdfs:subClassOf chemrof:Atom ;
    skos:definition "An atom type that only has a subset of properties (has atomic number, charge, and neutron number) stated" .

chemrof:PeriodicTableBlock a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "periodic table block" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:PeriodicTableBlockEnum ;
            owl:onProperty chemrof:type ],
        chemrof:PeriodicTablePlacement ;
    skos:definition "A block of the periodic table is a set of elements unified by the orbitals their valence electrons or vacancies lie in" .

chemrof:PeriodicTableGroup a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "periodic table group" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:int ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] .

chemrof:PreciseChemicalMixture a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "precise chemical mixture" ;
    rdfs:subClassOf chemrof:ChemicalMixture,
        chemrof:WeaklyBonded ;
    skos:altLabel "chemical substance (SIO)" ;
    skos:definition "A polyatomic entity that consists of two or more parts that are weakly bound, and of known stoichiometry" ;
    skos:exactMatch SIO:011126 .

chemrof:Reaction a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "reaction" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Boolean ;
            owl:onProperty chemrof:is_fully_characterized ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemrof:ReactionParticipant ;
            owl:onProperty chemrof:left_participants ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:reaction_center ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Boolean ;
            owl:onProperty chemrof:is_diastereoselective ],
        [ a owl:Restriction ;
            owl:onClass chemrof:ChemicalEncoding ;
            owl:onProperty chemrof:has_rinchi_representation ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Boolean ;
            owl:onProperty chemrof:is_transport ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:direction ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemrof:ReactionParticipant ;
            owl:onProperty chemrof:right_participants ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:smarts_string ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Boolean ;
            owl:onProperty chemrof:is_balanced ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEncoding ;
            owl:onProperty chemrof:has_reaction_smiles_representation ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:description ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Boolean ;
            owl:onProperty chemrof:is_stereo ] ;
    skos:definition "reaction" .

chemrof:Stereocenter a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "stereocenter" ;
    rdfs:subClassOf chemrof:Location,
        chemrof:OwlClass ;
    skos:altLabel "stereogenic center" ;
    skos:definition "a particular instance of a stereogenic element that is geometrically a point, bearing different substituents, such that interchanging any two substituents leads to a stereoisomer" .

chemrof:SubatomicParticle a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "subatomic particle" ;
    rdfs:subClassOf chemrof:ChemicalEntity ;
    skos:definition "A chemical entity below the granularity of an atom." ;
    skos:exactMatch CHEBI:36342,
        COB:0000007 .

chemrof:has_element a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has element" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalElement ;
    rdfs:subPropertyOf chemrof:alternate_form_of .

chemrof:has_repeat_unit a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has repeat unit" ;
    rdfs:range chemrof:ChemicalEntity .

chemrof:has_sequence_representation a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has sequence representation" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:SequenceString ;
    rdfs:subPropertyOf chemrof:chemical_representation .

chemrof:has_submolecules a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "has submolecules" ;
    rdfs:domain chemrof:Molecule ;
    rdfs:range chemrof:Molecule ;
    rdfs:subPropertyOf chemrof:has_part ;
    skos:definition "Relation between a molecule and the molecules it is made from." .

chemrof:inchi_atom_connections_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi atom connections sublayer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer_in_main_layer ;
    skos:definition "The atoms in the chemical formula (except for hydrogens) are numbered in sequence; this sublayer describes which atoms are connected by bonds to which other ones." .

chemrof:inchi_chemical_sublayer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi chemical sublayer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer_in_main_layer ;
    skos:definition "the main chemical formula" .

chemrof:inchi_sublayer_in_main_layer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inchi sublayer in main layer" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:inchi_sublayer ;
    skos:definition "The main layer consists of chemical formula, atom connections, and hydrogen atoms. Example: CH3O/c1-2/h1H3/q-1" .

chemrof:smiles_string a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "smiles string" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEncoding ;
    rdfs:subPropertyOf chemrof:chemical_formula ;
    skos:definition "A string encoding of a molecular graph, no chiral or isotopic information. There are usually a large number of valid SMILES which represent a given structure. For example, CCO, OCC and C(O)C all specify the structure of ethanol." ;
    skos:note "a problematic set of structures to describe using SMILES notation is those which cannot be easily described using molecular graphs (see “Limitations of molecular graph representations” section), such as organometallic compounds and ionic salts." .

chemrof:subatomic_particle_count a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "subatomic particle count" ;
    rdfs:domain chemrof:Atom ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:property_of_atom .

bo:symbol a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "symbol" ;
    rdfs:range linkml:String ;
    rdfs:subPropertyOf chemrof:information_property ;
    skos:definition "short symbol for chemical entity, e.g. K, C-16" ;
    skos:exactMatch bo:symbol .

chemrof:Charged a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "charged" ;
    rdfs:subClassOf chemrof:ChargeState,
        linkml:mixin ;
    skos:altLabel "ion",
        "ionic state" .

chemrof:Enantiomer a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "enantiomer" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:Molecule ;
            owl:onProperty chemrof:enantiomer_form_of ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:optical_configuration ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:absolute_configuration ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:relative_configuration ],
        chemrof:Stereoisomer ;
    skos:definition "one of two stereoisomers of a chiral molecule that are mirror images. Example: R-thalidomide" ;
    skos:exactMatch SIO:010777 ;
    skos:note "key= (enantiomer form of, absolute configuration)",
        "key= (enantiomer form of, inchi tetrahedral stereochemical sublayer)" .

chemrof:Material a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "material" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEntity ;
            owl:onProperty chemrof:has_part ] ;
    skos:definition "A substance composed of one or more chemical entities" .

chemrof:PolyatomicEntity a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "polyatomic entity" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemrof:inchi_atom_connections_sublayer ],
        chemrof:ChemicalEntity ;
    skos:definition "Any chemical entity consisting of more than one atom." .

chemrof:ReactionParticipant a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "reaction participant" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:stoichiometry ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:ChemicalEntity ;
            owl:onProperty chemrof:participant ],
        chemrof:RelationalChemicalEntity ;
    skos:definition "Instances of this link a reaction to a chemical entity participant" .

chemrof:conjugate_acid_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "conjugate acid of" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEntity ;
    rdfs:seeAlso "https://academic.oup.com/nar/article/36/suppl_1/D344/2506390" ;
    rdfs:subPropertyOf chemrof:direct_conjugatated_form_of,
        chemrof:has_higher_charge_from ;
    owl:inverseOf chemrof:conjugate_base_of ;
    skos:altLabel "is conjugate acid of (chebi)" .

chemrof:conjugate_base_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "conjugate base of" ;
    rdfs:domain chemrof:ChemicalEntity ;
    rdfs:range chemrof:ChemicalEntity ;
    rdfs:seeAlso "https://academic.oup.com/nar/article/36/suppl_1/D344/2506390" ;
    rdfs:subPropertyOf chemrof:direct_conjugatated_form_of,
        chemrof:has_lower_charge_from ;
    owl:inverseOf chemrof:conjugate_acid_of ;
    skos:altLabel "is conjugate base of (chebi)" ;
    skos:definition "connects the base form of an acid to an acid. Also connects the different charge states of the base. Example: the different charge states of lactate (CHEBI:24996) conjugate base of lactic acid (CHEBI:28358)" ;
    skos:editorialNote "sometimes chebi hops over a state skipping an intermediate" .

chemrof:polymer_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "polymer of" ;
    rdfs:range chemrof:Macromolecule ;
    rdfs:subPropertyOf chemrof:has_part ;
    skos:exactMatch CHEBI:60027,
        wd:P4600 .

chemrof:structural_isomer_of a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "structural isomer of" ;
    rdfs:domain chemrof:Molecule ;
    rdfs:range chemrof:Molecule ;
    rdfs:seeAlso "https://en.wikipedia.org/wiki/Structural_isomer",
        "https://goldbook.iupac.org/terms/view/C01285" ;
    rdfs:subPropertyOf chemrof:chemical_isomer_of ;
    skos:altLabel "constitutional isomer" ;
    skos:definition "a relationship that holds between two forms of the 'same' molecule with identical chemical formulas but different bonding patterns" .

chemrof:AcidBase a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "acid base" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:BronstedAcid ;
            owl:onProperty chemrof:has_acid_form ],
        chemrof:MolecularAnion ;
    skos:definition "A conugate base of an acid with charge specified. Example: citrate(3-)" .

chemrof:AtomOccurrence a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "atom occurrence" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass chemrof:Atom ;
            owl:onProperty chemrof:occurrence_of ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:formal_charge ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:oxidation_number ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:valence ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:coordination_number ],
        chemrof:OwlClass,
        chemrof:RelationalChemicalEntity ;
    skos:definition "An occurrence of an atom in the context of a particular molecule. For example, one of two occurrences of oxygen in nitrate" .

chemrof:AtomicBond a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "atomic bond" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Float ;
            owl:onProperty chemrof:torsional_angle ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Float ;
            owl:onProperty chemrof:bond_angle ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemrof:BondTypeEnum ;
            owl:onProperty chemrof:bond_type ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Float ;
            owl:onProperty chemrof:bond_length_in_angstroms ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemrof:AtomOccurrence ;
            owl:onProperty chemrof:has_atom_occurrences ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty chemrof:bond_order ],
        chemrof:OwlClass,
        chemrof:RelationalChemicalEntity ;
    skos:altLabel "bond",
        "chemical bond" ;
    skos:definition "A connection between two atoms. Note this is the reified form of 'atomically connected to'. " ;
    skos:exactMatch gc:Bond,
        CHEMINF:000063 .

chemrof:ChemicalElement a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "chemical element" ;
    rdfs:seeAlso "https://en.wikipedia.org/wiki/Chemical_element" ;
    rdfs:subClassOf [ a owl:Restriction ;

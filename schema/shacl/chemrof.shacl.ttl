chemrof:AcidAnionGroupingClass a sh:NodeShape ;
    sh:closed true ;
    sh:description "A molecule grouping class that groups the different ion forms of the base of an acid" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class owl:Class ;
            sh:description "Holds between C and P if C owl:subClassOf P. Note that in this model, most classes are metaclasses (see 'owl class'), where these metaclasses may represent *groupings* or specific countable elements or species. We provide two different sub-properties of subclass of for two cases: (1) subtype_of, which connects grouping classes (e.g. alkane is_a hydrocarbon; alpha-amino-acid is_a amino-acid), (2) classified_by, which connects a specific element to a grouping class (e.g. methane is_a hydrocarbon; iron is_a metal; serine is_a amino-acid)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path chemrof:owl_subclass_of ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path chemrof:id ],
        [ sh:class chemrof:ChemicalEntity ;
            sh:description "connects the base form of an acid to an acid. Also connects the different charge states of the base. Example: the different charge states of lactate (CHEBI:24996) conjugate base of lactic acid (CHEBI:28358)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path chemrof:conjugate_base_of ],
        [ sh:datatype xsd:string ;
            sh:description "name of chemical entity. E.g. nickel, carbon-16" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path rdfs:label ;
            sh:pattern "ate$" ],
        [ sh:class chemrof:ChemicalEntity ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path chemrof:conjugate_acid_of ],
        [ sh:class chemrof:MoleculeGroupingClass ;
            sh:description "grouping classes can be arranged in hierarchies arbitrarily deep. Note that subtype_of should only connect two grouping classes" ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path chemrof:subtype_of ] ;
    sh:targetClass chemrof:AcidAnionGroupingClass .

chemrof:AcidBase a sh:NodeShape ;
    sh:closed true ;
    sh:description "A conugate base of an acid with charge specified. Example: citrate(3-)" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "number of protons minus number of electrons" ;
            sh:maxCount 1 ;
            sh:maxInclusive -1 ;
            sh:order 19 ;
            sh:path chemrof:elemental_charge ],
        [ sh:datatype xsd:string ;
            sh:description "the main chemical formula" ;
            sh:maxCount 1 ;
            sh:order 12 ;
            sh:path chemrof:inchi_chemical_sublayer ;
            sh:pattern "^[A-Z0-9\\.]+$" ],
        [ sh:datatype xsd:string ;
            sh:description "name of chemical entity. E.g. nickel, carbon-16" ;
            sh:maxCount 1 ;
            sh:order 17 ;
            sh:path rdfs:label ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 16 ;
            sh:path chemrof:id ],
        [ sh:description "A chemical formula where numbers denote relative proportion of atoms. E.g. CH2O for glucose" ;
            sh:maxCount 1 ;
            sh:order 14 ;
            sh:path chemrof:empirical_formula ],
        [ sh:class chemrof:AtomOccurrence ;
            sh:description "An individuated atom in the context of a chemical structure." ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path chemrof:has_atom_occurrences ],
        [ sh:class chemrof:Molecule ;
            sh:description "Relation between a molecule and the molecules it is made from." ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path chemrof:has_submolecules ],
        [ sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
            sh:order 9 ;
            sh:path chemrof:is_radical ],
        [ sh:description "A string encoding of a molecular graph, no chiral or isotopic information. There are usually a large number of valid SMILES which represent a given structure. For example, CCO, OCC and C(O)C all specify the structure of ethanol." ;
            sh:order 13 ;
            sh:path chemrof:smiles_string ],
        [ sh:datatype xsd:string ;
            sh:description "The atoms in the chemical formula (except for hydrogens) are numbered in sequence; this sublayer describes which atoms are connected by bonds to which other ones." ;
            sh:maxCount 1 ;
            sh:order 7 ;
            sh:path chemrof:inchi_atom_connections_sublayer ;
            sh:pattern "^c.*" ],
        [ sh:class chemrof:ChemicalRole ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 10 ;
            sh:path chemrof:has_chemical_role ],
        [ sh:maxCount 1 ;
            sh:name "InChi" ;
            sh:order 11 ;
            sh:path chemrof:inchi_string ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:order 8 ;
            sh:path CHEMINF:000107 ],
        [ sh:class chemrof:ChemicalEntity ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path chemrof:conjugate_acid_of ],
        [ sh:maxCount 1 ;
            sh:order 18 ;
            sh:path chemrof:type ],
        [ sh:class owl:Class ;
            sh:description "Holds between C and P if C owl:subClassOf P. Note that in this model, most classes are metaclasses (see 'owl class'), where these metaclasses may represent *groupings* or specific countable elements or species. We provide two different sub-properties of subclass of for two cases: (1) subtype_of, which connects grouping classes (e.g. alkane is_a hydrocarbon; alpha-amino-acid is_a amino-acid), (2) classified_by, which connects a specific element to a grouping class (e.g. methane is_a hydrocarbon; iron is_a metal; serine is_a amino-acid)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 15 ;
            sh:path chemrof:owl_subclass_of ],
        [ sh:datatype xsd:boolean ;
            sh:description "An organic molecule is a molecule composed of organic atoms (at least carbon, hydrogen, and optionally oxygen, phosphorus, nitrogen, sulfur) [SIO]" ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path chemrof:is_organic ],
        [ sh:class chemrof:Atom ;
            sh:description "Relation between a molecule and the atoms it contains. Note that this is a shortcut relation: for granular representations go via atom occurrences" ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path chemrof:has_atoms ],
        [ sh:class gc:Bond ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path chemrof:has_bonds ],
        [ sh:class chemrof:ChemicalEntity ;
            sh:description "connects the base form of an acid to an acid. Also connects the different charge states of the base. Example: the different charge states of lactate (CHEBI:24996) conjugate base of lactic acid (CHEBI:28358)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path chemrof:conjugate_base_of ] ;
    sh:targetClass chemrof:AcidBase .


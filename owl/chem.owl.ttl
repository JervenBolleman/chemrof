chemschema:Molecule a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "molecule" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Boolean ;
            owl:onProperty chemschema:is_organic ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemschema:Atom ;
            owl:onProperty chemschema:has_atoms ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemschema:Molecule ;
            owl:onProperty chemschema:has_submolecules ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemschema:has_part ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemschema:AtomicBond ;
            owl:onProperty chemschema:has_bonds ],
        [ a owl:Restriction ;
            owl:allValuesFrom chemschema:AtomOccurrence ;
            owl:onProperty chemschema:has_atom_occurrences ],
        chemschema:FullyCovalentlyBonded,

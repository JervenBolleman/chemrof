        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemschema:ChemicalEncoding ;
            owl:onProperty chemschema:inchi_string ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemschema:ChemicalRole ;

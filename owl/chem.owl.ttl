chemschema:Atom a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "atom" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemschema:Count ;
            owl:onProperty chemschema:atomic_number ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass chemschema:SubatomicParticle ;
            owl:onProperty chemschema:has_part ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty chemschema:symbol ],
        chemschema:ChemicalEntity ;
    skos:altLabel "atomic species",
        "chemical species of an element" ;
    skos:definition "A chemical entity consisting of exactly one atomic nucleus and the electron(s) orbiting it." ;
    skos:exactMatch COB:0000011,
        SIO:010044 ;
    skos:note "{'chemical species of an element': 'Specific form of an element defined as to isotopic composition, electronic or oxidation state, and/or complex or molecular structure https://goldbook.iupac.org/terms/view/CT06859'}" .

chemschema:Isotope a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "isotope" ;

    rdfs:label "name" ;
    rdfs:range <https://w3id.org/biolink/biolinkml/meta/types/String> ;
    skos:definition "name of chemical entity. E.g. nickel, carbon-16" .

<http://w3id.org/chemontneutron_number> a owl:ObjectProperty,
        meta:SlotDefinition ;

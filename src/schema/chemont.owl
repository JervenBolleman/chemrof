            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/chemontMolecule> ;
            owl:onProperty meta:enantiomer_form_of ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <https://w3id.org/biolink/biolinkml/meta/types/String> ;
            owl:onProperty meta:optical_configuration ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <https://w3id.org/biolink/biolinkml/meta/types/String> ;
            owl:onProperty meta:absolute_configuration ],
        <http://w3id.org/chemontMolecule> ;

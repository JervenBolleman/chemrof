    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://w3id.org/chemontAtom> ;
            owl:onProperty meta:of_atom ],
        <http://w3id.org/chemontPattern> ;
